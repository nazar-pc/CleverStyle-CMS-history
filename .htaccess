AddDefaultCharset utf-8
php_flag display_errors on
 php_value error_reporting 2047
Options -All -Multiviews +FollowSymLinks
IndexIgnore *.php *.pl *.cgi *.htaccess *.htpasswd

RewriteEngine On
RewriteRule .* index.php

<Files *.jpg> 
	RewriteEngine Off
</Files>
<Files license.txt> 
	RewriteEngine Off
</Files>
<Files Storage.php> 
	RewriteEngine Off
</Files>
<Files readme.html> 
	RewriteEngine Off
</Files>
